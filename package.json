{
  "name": "katcafe-backend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "webpack --config build_webpack/webpack.terminal.config.js ",
    "start": "npm run build && node dist_bundle/terminal-bundle.js",
    "test": "webpack --config build_webpack/tests/webpack.terminal.test.config.js && mocha --exit --timeout 10000 dist_bundle/terminal-mocha-bundle.js --reporter mochawesome && npma run show_mocha_report",
    "show_mocha_report": "node src/tests/browser/show-mocha.test.js",
    "coverage": "npm run build_terminal  && istanbul cover _mocha dist/tests/main.test.js -x *.test.js && npm run show_coverage",
    "show_coverage": "node src/tests/browser/show-coverage.test.js"
  },
  "dependencies": {
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-relative-import": "^1.0.3",
    "bluebird": "^3.5.2",
    "body-parser": "~1.17.1",
    "cheerio": "^1.0.0-rc.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.3",
    "debug": "~2.6.3",
    "express": "~4.15.2",
    "image-size": "^0.7.4",
    "multer": "^1.3.0",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "salted-md5": "^4.0.2",
    "sanitize-html": "^1.20.1",
    "sharp": "^0.22.1",
    "sitemap-generator": "^8.4.0",
    "svg-captcha": "^1.4.0",
    "xregexp": "^4.2.4",
    "youtube-info": "^1.3.2"
